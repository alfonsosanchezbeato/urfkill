#. TRANSLATORS: exit after we've started up, used for user profiling
#: ../src/urf-main.c:146
msgid "Exit after a small delay"
msgstr "Terminer après un délai"

#. TRANSLATORS: exit straight away, used for automatic profiling
#: ../src/urf-main.c:149
msgid "Exit after the engine has loaded"
msgstr "Terminer après le démarrage de l'engin"

#. TRANSLATORS: fork to background
#: ../src/urf-main.c:152
msgid "Fork on startup"
msgstr "Passer en arrière-plan au démarrage"

#. TRANSLATORS: change to another user and drop the privilege
#: ../src/urf-main.c:155
msgid "Use a specific user instead of root"
msgstr "Changer à l'utilisateur spécifié"

#. TRANSLATORS: use another config file instead of the default one
#: ../src/urf-main.c:158
msgid "Use a specific config file"
msgstr "Utiliser le fichier de configuration spécifié"

#. TRANSLATORS: enable debug logging
#: ../src/urf-main.c:161
msgid "Enable debug logging"
msgstr "Activer la fonction de déboguage"
